{
    "name": "brunofalcaodev/nidavellir-trading",
    "description": "Nidavellir Trading",
    "keywords": [
        "brunofalcaodev"
    ],
    "homepage": "https://github.com/brunofalcaodev/nidavellir-trading",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "me@brunofalcao.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "brunofalcaodev/nidavellir-cube": "dev-master",
        "brunofalcaodev/nidavellir-abstracts": "dev-master",
        "brunofalcaodev/nidavellir-apis": "dev-master",
        "brunofalcaodev/nidavellir-exceptions": "dev-master",
        "brunofalcaodev/nidavellir-jobs": "dev-master",
        "brunofalcaodev/nidavellir-webhooks": "dev-master",
        "brunofalcaodev/nidavellir-commands": "dev-master",
        "brunofalcaodev/nidavellir-workflows": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nidavellir\\Trading\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nidavellir\\Trading\\NidavellirTradingServiceProvider"
            ]
        }
    }
}
